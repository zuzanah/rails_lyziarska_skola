<p id="notice"><%= notice %></p>

<h1>Skinews</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th>Vlozene</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skinews.each do |skinews| %>
      <tr>
        <td><%= skinews.title %></td>
        <td><%= skinews.body %></td>
        <td><%= skinews.created_at.strftime("%b %d, %Y") %></td>
        <td><%= link_to 'Show', skinews %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Back', root_path %>
